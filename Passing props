      <Heading firstName="Any name other than Bob" />
    </div>
  );
}

export default App;


import Heading from "./Heading";

function App() {
  return (
    <div className="App">
      <Heading firstName="Any name other than Bob" />
      <Heading firstName="Jack" />
    </div>
  );
}

export default App;

